# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

->          /                                          core.Routes

GET         /login                                     controllers.Application.login
GET         /logout                                    controllers.Application.logout
GET         /auth/:strategy                            controllers.Application.auth(strategy)

GET         /custom/artifactList                       controllers.Application.artifactList
POST        /ddl/:tableName                            controllers.Application.ddl(tableName)
POST        /import/:module/:artifactType/:name        controllers.Application.importArtifact(module, artifactType, name)
POST        /export/:module/:artifactType/:name        controllers.Application.exportArtifact(module, artifactType, name)

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file                              controllers.Assets.at(path="/public", file)
